
{
  "name": "SLG-MD",
  "description": "Un bot WhatsApp MD construit en Node.js pour améliorer l'expérience",
  "logo": "https://example.com/logo.png",
  "keywords": ["nodejs", "bot", "whatsapp bot", "SLG", "SLG-MD", "Sequelize", "fs", "dotenv"],
  "success_url": "/",
  "stack": "container",
  "env": {
    "ANTILINK": {
      "description": "Activer ou désactiver l'antilink",
      "value": "false"
    },
    "LOGS": {
      "description": "Activer ou désactiver les journaux",
      "value": "true"
    },
    "ANTILINK_ACTION": {
      "description": "Action à effectuer pour l'antilink",
      "value": "kick"
    },
    "LANG": {
      "description": "Langue du bot",
      "value": "EN"
    },
    "HANDLERS": {
      "description": "Gestionnaires",
      "value": "[#]"
    },
    "RMBG_KEY": {
      "description": "Clé RMBG",
      "required": false
    },
    "BRANCH": {
      "description": "Branche",
      "value": "main"
    },
    "WARN_COUNT": {
      "description": "Nombre d'avertissements",
      "value": "3"
    },
    "PACKNAME": {
      "description": "Nom du pack",
      "value": "X-Asena"
    },
    "WELCOME_MSG": {
      "description": "Message de bienvenue",
      "value": "Hi @user Welcome to @gname"
    },
    "GOODBYE_MSG": {
      "description": "Message d'au revoir",
      "value": "Hi @user It was Nice Seeing you"
    },
    "AUTHOR": {
      "description": "Auteur",
      "value": "X-Electra"
    },
    "SUDO": {
      "description": "Numéros des utilisateurs sudo",
      "value": "918113921898,919598157259,918590508376,919383400679"
    },
    "HEROKU_APP_NAME": {
      "description": "Nom de l'application Heroku",
      "required": false
    },
    "HEROKU_API_KEY": {
      "description": "Clé API Heroku",
      "required": false
    },
    "OWNER_NAME": {
      "description": "Nom du propriétaire",
      "value": "SLG"
    },
    "HEROKU": {
      "description": "Bot sur Heroku ou non",
      "value": "false"
    },
    "BOT_NAME": {
      "description": "Nom du bot",
      "value": "SLG-MD"
    },
    "AUTO_READ": {
      "description": "Lire automatiquement les messages",
      "value": "false"
    },
    "AUTO_STATUS_READ": {
      "description": "Lire automatiquement les statuts",
      "value": "false"
    },
    "PROCESSNAME": {
      "description": "Nom du processus",
      "value": "SLG"
    },
    "WORK_TYPE": {
      "description": "Type de travail du bot",
      "value": "private"
    },
    "DELETED_LOG": {
      "description": "Activer ou désactiver les journaux de suppression de message",
      "value": "false"
    },
    "REMOVEBG": {
      "description": "Activer ou désactiver Remove.bg",
      "value": "false"
    },
    "DATABASE_URL": {
      "description": "URL de la base de données",
      "value": "./assets/database.db"
    },
    "STATUS_SAVER": {
      "description": "Activer ou désactiver l'enregistrement des statuts",
      "value": "true"
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "type": "web",
      "command": "npm start"
    }
  }
}